<form action="" method="post">
    <div class="mb-3 container">
      <label for="exampleFormControlInput1" class="form-label">ID</label>
      <input 
        type="ID"
        name="ID"
        value="<%= user.ID %>"
        class="form-control" 
        id="exampleFormControlInput1" 
        placeholder="Input ID"
        />
    </div>
    <div class="mb-3 container">
      <label for="exampleFormControlInput1" 
        class="form-label">
        >NAMA</label
      >
      <input 
        type="NAMA" 
        name="NAMA"
        value="<%= user.NAMA %>"
        class="form-control" 
        id="exampleFormControlInput1" 
        placeholder="Input NAMA"
        />
    </div>
    <div class="mb-3 container">
      <label for="exampleFormControlInput1" 
        class="form-label">
        >NIT</label
      >
      <input 
        type="NIT" 
        name="NIT"
        value="<%= user.NIT %>"
        class="form-control" 
        id="exampleFormControlInput1" 
        placeholder="Input NIT"
      />
    </div>
    <div class="mb-3 container">
      <label for="exampleFormControlInput1" 
        class="form-label"
        >NOTELP</label
      >
      <input 
        type="NOTELP" 
        name="NOTELP"
        value="<%= user.NOTELP %>"
        class="form-control" 
        id="exampleFormControlInput1" 
        placeholder="Input NOTELP"
      />
    </div>
    <div class="modal-footer">
      <button 
      type="button" 
      class="btn btn-secondary" 
      data-bs-dismiss="modal"
    >
      Close
    </button>
    <button type="submit" class="btn btn-primary">
      Save changes
    </button>
    </div>
  </form>